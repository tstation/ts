                <script>
                   $(function() {
                    var homeSwiper = new Swiper('.deal-swiper', {
                        navigation: {
                            nextEl: '.swiper-button-next',
                            prevEl: '.swiper-button-prev',
                        },
                        // spaceBetween: 15,
                        pagination: {
                            el: '.swiper-pagination',
                            clickable: true,
                        },
                        autoplay: {
                            delay: 4000,
                            disableOnInteraction: false,
                        },
                        loop: true,
                        autoHeight: true
                    });

                    var homeSwiperPlay = true;
                    var homePlayButton =  $(".swiper-button-play");
                    var homeStopButton =  $(".swiper-button-pause");

                    homeStopButton.addClass('active');
                    homePlayButton.click(function(){
                        homeSwiper.autoplay.start();
                        homeSwiperPlay = true;
                        playButtonActive();
                        console.log(homeSwiperPlay);
                    });

                    homeStopButton.click(function(){
                        homeSwiper.autoplay.stop();
                        homeSwiperPlay = false;
                        playButtonActive();
                        console.log(homeSwiperPlay);
                    });

                    function playButtonActive() {
                        if(homeSwiperPlay) {
                            homeStopButton.addClass('active');
                            homePlayButton.removeClass('active');
                        } else {
                            homePlayButton.addClass('active');
                            homeStopButton.removeClass('active');
                        }   
                    }
                });
                </script>

            </div>
