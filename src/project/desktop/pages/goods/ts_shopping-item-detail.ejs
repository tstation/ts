                /*  tab function and anchor*/
                $(document).ready(function() {
                    var itemDetail = $('.item-tab-section');
                    var tabButton = itemDetail.find('.tablist > .button-tab');
                    var tabContainer = itemDetail.find('.tab_cont');

                    tabButton.each(function(i) {
                        tabButton.eq(i).click(function(){
                            tabAnchor = itemDetail.find('.tablist-wrap').offset().top;
                            $('html, body').scrollTop(tabAnchor);
                            tabContainer.hide();
                            tabContainer.eq(i).show();
                            tabButton.removeClass('active');
                            tabButton.eq(i).addClass('active');
                        });
                    });
                });

                /* relatedSwiper */
                var relatedCard = $('.related-swiper-wrap .swiper-slide').length;
                if( relatedCard < 4 ){
                    $('.related-swiper-wrap').addClass('no-swipe');
                }else{
                    var mycar_select_swiper = new Swiper('.related-swiper-wrap', {
                        init: false,
                        spaceBetween: 0,
                        slidesPerView: 4,
                        slidesPerGroup: 4,
                        navigation: {
                            nextEl: '.item-related-section .btn-swiper-nav.btn-next',
                            prevEl: '.item-related-section .btn-swiper-nav.btn-prev',
                        },
                    });
                    mycar_select_swiper.init();
                };
            });
        </script>
    </div><!-- // .item-detail end -->








