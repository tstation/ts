            $(document).ready(function(){
                /* keyVisual sticky scroll */
                $(window).on({
                    'scroll' : function() {
                        var stickyWrap = $('.sticky-wrap')
                        var paymentTop = $('.item-section').offset().top;
                        var maxTop = $('.item-info-wrap').outerHeight() - stickyWrap.outerHeight() + paymentTop;
                        var scrollThis = $(this).scrollTop();

                        if ( paymentTop < scrollThis ) {
                            if (scrollThis < maxTop) {
                                stickyWrap.css({'top' : scrollThis-paymentTop});
                            } else {
                                stickyWrap.css({'top' : maxTop-paymentTop});
                            }
                        } else {
                            stickyWrap.css({'top' : 0});
                        }
                    }
                });

                /* good tablist sticky scroll */
                $(window).on({
                    'scroll' : function() {
                        var tablistPos = $('.tablist-wrap').offset().top;
                        var scrollPos = $(this).scrollTop();

                        if(tablistPos < scrollPos) {
                            $('.tablist').addClass('fixed');
                        } else {
                            $('.tablist').removeClass('fixed');
                        }
                    }
                });

                /* good detail click event hide */
                $('.detail-gradient').find('button').click(function(){
                    var goodDetail = $('#goodDetail_tab1')
                    if(goodDetail.hasClass('hidden')) {
                        goodDetail.removeClass('hidden');
                        goodDetail.find('.vendor-container').hide();
                    }
                });

                /* toogle event */
                $('.toggle-wrap').find('.headline').click(function() {
                    var toggleWrap = $(this).parent('.toggle-wrap');
                    if(toggleWrap.hasClass('active')) {
                        toggleWrap.removeClass('active');
                    } else {
                        toggleWrap.addClass('active');
                    }

                    // 높이 업데이트
