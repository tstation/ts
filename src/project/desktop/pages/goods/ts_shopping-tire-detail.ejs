                    var itemDetail = $('.item-tab-section');
                    var tabButton = itemDetail.find('.tablist > .button-tab');
                    var tabContainer = itemDetail.find('.tab_cont');

                    tabButton.each(function(i) {
                        tabButton.eq(i).click(function(){
                            tabAnchor = itemDetail.find('.tablist-wrap').offset().top;
                            $('html, body').scrollTop(tabAnchor);
                            tabContainer.hide();
                            tabContainer.eq(i).show();
                            tabButton.removeClass('active');
                            tabButton.eq(i).addClass('active');
                        });
                    });
                });

