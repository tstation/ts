        var callPopup = {
            // 주문하기
            order: function () {
                $.Tstation.popup().open({
                    ref: 'order',
                    title: '구매방법',
                    requestUrl: '', // 비동기 방식으로 컨텐츠 영역을 받아올때
                    contents: popupTemplate.order, // 팝업을 바로 띄우고 싶을때
                    dimWhite: false,
                    modalContentsPd: {
                        horizon: 5,
                        vertical: 0,
                        top: 3,
                        right: 0,
                        bottom: 6,
                        left: 0
                    },
                    modalSize: 'MEDIUM'
                })
            },
        }

        // filter
        function eqAnyOf(target, arrayOfIndexes) {
            return target.filter(function(i) {
                return $.inArray(i, arrayOfIndexes) > -1;
            });
        }

        // 매장 리스트 상태 변경
        var sortShopList = function (condition) {
            // sample condition { el: element, type: 'TODAY / ALL' }
            var _this = $(condition.el)

            // 1. toggle button
            _this
              .siblings()
              .removeClass(constants.className.btnPrimary)
              .addClass(constants.className.btnGray)
            _this
              .addClass()
              .removeClass(constants.className.btnGray)
              .addClass(constants.className.btnPrimary)

            // 2. sort list
            switch (condition.type) {
                case 'TODAY':
                    eqAnyOf(_this.closest(constants.selector.tabContents).find(constants.selector.storeWrap), [0, 1, 2, 3]).addClass('disable');
                    break
                case 'ALL':
                    _this
                      .closest(constants.selector.tabContents)
                      .find(constants.selector.storeWrap)
                      .removeClass(constants.className.disable)
                    break
                default:
                    throw new Error('sort shop require type')
            }
        }

        var toggleShopView = function (condition) {
            // sample condition { el: element, type: 'MAP / LIST' }
            var _this = $(condition.el)
            var tabContents = _this.closest(constants.selector.tabContents)

            // 1. toggle button
            _this.hide().siblings().show();

            // 2. toggle shop view
            switch (condition.type) {
                case 'MAP':
                    tabContents.find(constants.selector.storeMap).show()
                    tabContents.find(constants.selector.storeList).hide()
                    break
                case 'LIST':
                    tabContents.find(constants.selector.storeList).show()
                    tabContents.find(constants.selector.storeMap).hide()
                    break
                default:
                    throw new Error('toggle shop view require type')
            }
        }


        /**
         * 상품 카드 주문하기 선택
         **/
        $(document).on('change', '.sale-check input[type="radio"]', function (evt) {
            var target = $(evt.target)
            console.log(target)
            callPopup.order();
        })


        /**
         * 구매 강화 프로세스 바 기능 구현 스크립트
         **/
        var selector = {
            tsShoppingSection: '.ts-shopping-section-swiper',
            processBar: '.product-buy-process-bar',
            processItem: '.product-buy-process-item',
            done: '.confirm-done',
            yet: '.confirm-done',
            car: '.car',         // 차량/사이즈 검색
            shop: '.shop',       // 장착점 검색
            order: '.order',     // 주문하기
        }
        var className = {
            done: 'done', // 차량,타이어 선택 및 장착점 선택을 완료
            checkView: 'check-view'
        }
        // 구매 강화 프로세스
        var bottomBarAction = {
            car: {
                call: function (noSelected) {
                    if (noSelected) {
                        callPopup.searchByCar();
                        return
                    }
                    // 1. 선택된 차량정보가 있는지 확인하여 없다면 callPopup.findMyCar(), 있다면 callPopup.searchByCar()
                    alert('선택된 차량정보가 존재하거나 존재하지 않는 상황에 대해 랜덤으로 보여줍니다.')
                    Math.random() >= 0.5 ? callPopup.searchByCar() : callPopup.findMyCar();
                },
                change: function () {
                    var car = $(selector.processBar + ' ' + selector.processItem + selector.car)
                    // 1. product-buy-process-bar > product-buy-process-item car > confirm-done > info 바인딩
                    // 2. product-buy-process-bar > product-buy-process-item car > confirm-done 보이기
                    car.addClass(className.done);
                    // 3. 팝업 닫기
                    $.Tstation.popup().close();
                },
                destroy: function () {}
            },
            shop: {
                call: function (noSelected) {
                    if (noSelected) {
                        callPopup.choiceShop();
                        return
                    }
                    // 1. 장착점 선택정보가 존재하지 않는다면 callPopup.choiceShop(), 존재 한다면 callPopup.choiceShopSelected()
                    alert('장착점 선택정보가 존재하거나 존재하지 않는 상황에 대해 랜덤으로 보여줍니다.')
                    Math.random() >= 0.5 ? callPopup.choiceShop() : callPopup.choiceShopSelected();
                },
                change: function () {
                    var shop = $(selector.processBar + ' ' + selector.processItem + selector.shop)
                    // 1. product-buy-process-bar > product-buy-process-item shop > confirm-done > info 바인딩
                    // 2. product-buy-process-bar > product-buy-process-item shop > confirm-done 보이기
                    shop.addClass(className.done)
                    // 3. 팝업 닫기
                    $.Tstation.popup().close();
                },
                destroy: function () {}
            },
            order: {
                toggleOn: function () {
                    var tsShoppingSection = $(selector.tsShoppingSection)

                    $.each(tsShoppingSection, function (index, item) {
                        $(item).toggleClass(className.checkView)
                    })
                },
                call: function () {
                    callPopup.order();
                }
            }
        }
    </script>
