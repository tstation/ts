        }
        var className = {
            done: 'done', // 차량,타이어 선택 및 장착점 선택을 완료
            checkView: 'check-view'
        }
        // 구매 강화 프로세스
        var bottomBarAction = {
            car: {
                call: function (noSelected) {
                    if (noSelected) {
                        callPopup.searchByCar();
                        return
                    }
                    // 1. 선택된 차량정보가 있는지 확인하여 없다면 callPopup.findMyCar(), 있다면 callPopup.searchByCar()
                    alert('선택된 차량정보가 존재하거나 존재하지 않는 상황에 대해 랜덤으로 보여줍니다.')
                    Math.random() >= 0.5 ? callPopup.searchByCar() : callPopup.findMyCar();
                },
                change: function () {
                    var car = $(selector.processBar + ' ' + selector.processItem + selector.car)
                    // 1. product-buy-process-bar > product-buy-process-item car > confirm-done > info 바인딩
                    // 2. product-buy-process-bar > product-buy-process-item car > confirm-done 보이기
                    car.addClass(className.done);
                    // 3. 팝업 닫기
                    $.Tstation.popup().close();
                },
                destroy: function () {}
            },
            shop: {
                call: function (noSelected) {
                    if (noSelected) {
                        callPopup.choiceShop();
                        return
                    }
                    // 1. 장착점 선택정보가 존재하지 않는다면 callPopup.choiceShop(), 존재 한다면 callPopup.choiceShopSelected()
                    alert('장착점 선택정보가 존재하거나 존재하지 않는 상황에 대해 랜덤으로 보여줍니다.')
                    Math.random() >= 0.5 ? callPopup.choiceShop() : callPopup.choiceShopSelected();
                },
                change: function () {
                    var shop = $(selector.processBar + ' ' + selector.processItem + selector.shop)
                    // 1. product-buy-process-bar > product-buy-process-item shop > confirm-done > info 바인딩
                    // 2. product-buy-process-bar > product-buy-process-item shop > confirm-done 보이기
                    shop.addClass(className.done)
                    // 3. 팝업 닫기
                    $.Tstation.popup().close();
                },
                destroy: function () {}
            },
            order: {
                toggleOn: function () {
                    var tsShoppingSection = $(selector.tsShoppingSection)

                    $.each(tsShoppingSection, function (index, item) {
                        $(item).toggleClass(className.checkView)
                    })
                },
                call: function () {
                    callPopup.order();
                }
            }
        }
