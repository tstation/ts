    </div>
</div>

<script>
    /**
     * CONSTANTS
     */
    var lpData = {
        cookie: '_lp-expire',
        classes: {
            lp: '#main-landing-popup',
            swiper: '.landing-popup-swiper'
        },
        lpExpire: false
    }

    /**
     * GET COOKIE
     */
    var getCookie = function (name) {
        var value = '; ' + document.cookie
        var parts = value.split(('; ' + name + '='))
        return parts.length === 2 ? parts.pop().split(';').shift() : ''
    }

    /**
     * SET COOKEY
     */
    var setCookie = function (name, value, days) {
        var date = new Date()
        date.setTime(date.getTime() + (days * 60 * 60 * 24 * 1000))
        document.cookie = name + '=' + escape(value) + ';expires=' + date.toUTCString() + ';path=/'
    }

    /**
     * LANDING POPUP EXPIRE UPDATE
     */
    var lpExpireUpdate = function (event) {
        lpData.lpExpire = event.currentTarget.checked
    }


    /**
     * LANDING POPUP SWIPER INIT
     * return void
     */
    var lpSwiperInit = function () {
        if($('.swiper-container').hasClass('landing-popup-swiper')) {
            var landingPopupSwiper = new Swiper(lpData.classes.swiper, {
                pagination: {
                    el: '.pagination-wrap > .swiper-pagination',
                    type: "fraction",
                },
                navigation: {
                                nextEl: ".swiper-next",
                                prevEl: ".swiper-prev",
                            },
                slidesPerView: 'auto',
                spaceBetween: 0,
            });

            landingPopupSwiper.init()

            $(window).on('resize', function () {
                landingPopupSwiper.init()
            })
        } else {
            return false;
        }
    }

    /**
     * LANDING POPUP CLOSE
     */
    var lpClose = function () {
        if (lpData.lpExpire) {
            setCookie(lpData.cookie, 'Y', 1)
        }
        $(lpData.classes.lp).remove()
    }


    /**
     * Rendering OR Destroy LANDING POPUP
     */
    $(document).ready(function () {
        var isLpShow = getCookie(lpData.cookie) !== 'Y'
        var lp = $(lpData.classes.lp)

        if (isLpShow) {
            lp.addClass('on')
        } else {
            lp.remove()
        }

        lpSwiperInit();
    })
</script>
