        <script>
        var mainKeyvisualWrap = ".key-visual-swiper";
	    var mainKeyvisualSwiperItems =  $(mainKeyvisualWrap).children(".swiper-wrapper").children();
	    var mainKeyvisualSwiperIndex = null;
	    var mainKevisualSwiperOption = {
	        // navigation : false,
	        // watchOverflow : true,
	        // slidesPerView: 1,
	        // speed: 0,
	        loop: true,
	        autoplay: {
	             delay : 8000, // default : image
	             disableOnInteraction: false,
	        },
	        effect: 'fade',
	        // fadeEffect: {
	        //     crossFade: true
	        // },
	        // centeredSlides: true,
	        // simulateTouch: false,
	        // pagination: {
	        //     el: '.swiper-pagination',
	        //     clickable : true,
	        //     renderBullet: function (index, className) {
	        //         return '<button class="' + className + '">sss</button>';
	        //     }
	        // },
	        on : {
	            init : function () {
	                var animateDelay = $(mainKeyvisualSwiperItems[0]).attr("data-swiper-delay");
	                var stick = $('.key-visual-button-area').find(".indicator-gauge .gauge"); 
	                stick.stop().animate({"width": "100%"}, Number(animateDelay));
	                console.log("swiper init : stick animation start");
                    kvIndicator(1);
	            },
	            activeIndexChange: function () {
	                mainKeyvisualSwiperIndex = this.realIndex;
	            },
	        }
	    }
	
	    var mainKeyvisual = new Swiper( mainKeyvisualWrap ,mainKevisualSwiperOption); // swiper 실행
        var stick = $('.key-visual-button-area').find(".indicator-gauge .gauge");
		var slideIndex = 0;

	    mainKeyvisual.on("slideChange", function () {
	    	mainKeyvisualSwiperItems.each(function (key){
  	            var animateDelay = $(mainKeyvisualSwiperItems[mainKeyvisualSwiperIndex]).attr("data-swiper-delay");
  	            var realIndexSlide = $(mainKeyvisualSwiperItems[mainKeyvisualSwiperIndex]);
  	            
                //console.log(key + "key");
                //console.log(mainKeyvisualSwiperIndex + "mainKeyvisualSwiperIndex");

  	            if(key === mainKeyvisualSwiperIndex){
  	            	stick.css({"width": 0});
  	            	stick.stop().animate({"width": "100%"}, Number(animateDelay));
  	
  	                if(realIndexSlide.attr("data-swiper-autoplay") !== undefined){
  	                	realIndexSlide.find("video")[0].currentTime = 0;
  					}
  	                
  	                //console.log("swiper change slide delay : ", animateDelay )
  	            }
  	        });   

            slideIndex = this.realIndex + 1;
            kvIndicator(slideIndex);
	    });

        function kvIndicator(slideIndex){
            $('.key-visual-console > li button').removeClass('on');
            $('.key-visual-console > li:nth-of-type('+ slideIndex +') button').addClass('on');
        }

        $('.key-visual-console > li button').on('click', function () {
            var index = $(this).closest('li').index();
            mainKeyvisual.slideToLoop(index);
            keyVisualPlayButton();
            console.log(index);
        });

         /* 20220527 // */
        var keyvisualStartButton = $(".swiper-start");
        var keyvisualStopButton = $(".swiper-stop");

        // 플레이어 버튼 init
        keyVisualPlayButton();

        // 버튼 클릭시 정지
        keyvisualStopButton.on('click', function() {
            keyVisualStopButton();
        })

        // 버튼 클릭시 시작
        keyvisualStartButton.on('click', function() {
            keyVisualPlayButton();
        })

        function keyVisualPlayButton() {
            // mainKeyvisual.autoplay.start();
            // keyvisualStartButton.hide();
            // keyvisualStopButton.show();
            // console.log("a");
        }

        function keyVisualStopButton() {
            // mainKeyvisual.autoplay.stop();
            // keyvisualStartButton.show();
