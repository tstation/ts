                    var scrollPos;
        
                    $(window).on('scroll', function(){
                        orderStickyContainerTop = $('.order-lower-area').offset().top;
                        orderStickyContainerH = $('.order-lower-area').outerHeight();
                        orderStickyAreaH = $('.order-sticky-area').outerHeight();
                        orderStickyGap = orderStickyContainerH - orderStickyAreaH;
                        scrollPos = $(window).scrollTop();
        
                        if( scrollPos < orderStickyContainerTop ){
                            $('.order-sticky-area').removeClass('sticky absolute');
                        }else{
                            if( scrollPos > orderStickyContainerTop + orderStickyGap ){
                                $('.order-sticky-area').removeClass('sticky');
                                $('.order-sticky-area').addClass('absolute');
                            }else{
