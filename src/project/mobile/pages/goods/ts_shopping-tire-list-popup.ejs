                        <!-- 4. 상품 검색 팝업 인터렉션 -->
                        <script>
                            /**
                             * 상품 검색 팝업 인터렉션
                             */
                            var modalConstants = {
                                selector: {
                                    modalItem: '.modal-item',
                                    onSearchModal: '.on-search-modal',
                                    modalHeader: '.modal-header',
                                    modalBody: '.modal-body',
                                    tabItem: '.tab-item'
                                },
                                className: { on: 'on' },
                                customAttr: {
                                    tabEvent: 'data-tab-event'
                                },
                                tabs: {
                                    carNumber: { backgroundColor: '#FFFFFF' },
                                    carType: { backgroundColor: '#FFFFFF' },
                                    size: { backgroundColor: '#F9F9FB' }
                                },
                                methods: {
                                    toCamelCase: function (str) {
                                        return str.toLowerCase()
                                          .replace(/-+(\w|$)/g, function ($$, $1) {
                                              return $1.toUpperCase();
                                          });
                                    },
                                    changeBgColor: function (el, target) {
                                        $(el).closest(modalConstants.selector.modalBody).css({ 'background-color': modalConstants.tabs[target].backgroundColor })
                                    },
                                    tabChangeTrigger: function (el, target) {
                                        switch (target) {
                                            case 'carNumber':
                                                break
                                            case 'carType':
                                                // 차종찾기
                                                setTimeout(function () {
                                                    var modelSearchSwiper = new Swiper('.model-search-swiper', {
                                                        spaceBetween: 4,
                                                        slidesPerView: 'auto',
                                                        observer: true,
                                                        observeParents: true,
                                                        freeMode: true
                                                    });

                                                    modelSearchSwiper.init();

                                                    $(window).on('resize', function () {
                                                        modelSearchSwiper.init();
                                                    });
                                                }, 10)
                                                break
                                            case 'size':
                                                break
                                        }
                                    }
                                }
                            }

                            // 탭 변경하면 모달 바디 배경색 지정
                            $(document).on('click', modalConstants.selector.onSearchModal + modalConstants.selector.tabItem + ' a', function () {
                                var tabEvent = $(this).data().tabEvent
                                if (typeof tabEvent === 'undefined') {
                                    return
                                }
                                // 1. background color changer
                                modalConstants.methods.changeBgColor(this, modalConstants.methods.toCamelCase(tabEvent))
                                // 2. trigger
                                modalConstants.methods.tabChangeTrigger(this, modalConstants.methods.toCamelCase(tabEvent))
                            })
                        </script>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

