                            <script>
                                var fastDate = 7; //16번째 .swiper-slide 순서
                                var monthDate = 10 //10월
                                var clickData = 0;
                                var elActive = $('.date-inner').find('.swiper-slide');
                                var elDayMonth = $('.store-calendar').find('.select-day');

                                var dateSwiper = new Swiper('.date-inner', {
                                    init: false,
                                    initialSlide: 5,
                                    spaceBetween: 4,
                                    slidesPerView: 'auto'
                                });

                                dateSwiper.init();

                                $(window).on('resize', function () {
                                    dateSwiper.init();
                                });

                                fastDateFunc(fastDate);
                                dataNumFunc(fastDate);

                                function fastDateFunc(fastDate) {
                                    dateSwiper.slideTo(fastDate);
                                    elActive.removeClass('on');
                                    elActive.eq(fastDate).addClass('on');
                                }

                                function dataNumFunc(data) {
                                    var dataNum = $('.date-inner').find('.swiper-slide > .date-num').eq(data).text();
                                    elDayMonth.text(monthDate+"월 "+dataNum+"일");
                                    console.log(elDayMonth);
                                }
                                
                                elActive.each(function(elData) {
                                    $(this).click(function() {
                                        clickData = elData;
                                        elActive.removeClass('on');
                                        elActive.eq(clickData).addClass('on');
                                        dataNumFunc(elData)
                                    })
                                })                                    

                                $('#btnReSelectShopDate').click(function() {
                                    dateSwiper.slideTo(0, 1000, false)
